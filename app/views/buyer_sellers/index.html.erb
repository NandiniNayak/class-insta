<p id="notice"><%= notice %></p>

<h1>Buyer Sellers</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Seller</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buyer_sellers.each do |buyer_seller| %>
      <tr>
        <td><%= buyer_seller.user %></td>
        <td><%= buyer_seller.seller %></td>
        <td><%= link_to 'Show', buyer_seller %></td>
        <td><%= link_to 'Edit', edit_buyer_seller_path(buyer_seller) %></td>
        <td><%= link_to 'Destroy', buyer_seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Buyer Seller', new_buyer_seller_path %>
